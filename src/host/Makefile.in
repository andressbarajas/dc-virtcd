CC = @CC@
AR = @AR@
CFLAGS = @CFLAGS@

top_srcdir=@top_srcdir@
srcdir=@srcdir@
VPATH=@srcdir@

all clean :
	make local-$@

local-all :

local-clean :

install :

Makefile: Makefile.in config.status
	./config.status

config.status : configure
	./config.status --recheck

configure : configure.ac
	cd $(srcdir) && autoconf

