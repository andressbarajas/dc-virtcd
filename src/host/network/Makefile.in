CC = @CC@
AR = @AR@
CFLAGS = @CFLAGS@ -I@top_builddir@ -I@top_srcdir@/common

top_srcdir=@top_srcdir@
srcdir=@srcdir@
VPATH=@srcdir@

OBJS = serverport.o

LIBTARGET = libnetwork.a

all : $(LIBTARGET)

clean :
	-rm -f $(LIBTARGET) $(OBJS)

$(LIBTARGET) : $(OBJS)
	$(AR) rc $@ $(OBJS)

serverport.o : serverport.c serverport.h @top_builddir@/config.h @top_srcdir@/common/msglog.h

